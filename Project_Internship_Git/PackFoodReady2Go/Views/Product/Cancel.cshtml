<body class="goto-here">


	<div class="vh-30 d-flex justify-content-center align-items-center m-5">
		<div class="col-md-4">
			<div class="border border-3 border-danger"></div>
			<div class="card  bg-white shadow p-5">
				<div class="mb-4 text-center">
					<svg xmlns="http://www.w3.org/2000/svg" class="text-danger bi bi-x-circle" width="75" height="75"
						 fill="currentColor" viewBox="0 0 16 16">
						<path fill-rule="evenodd" d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm3.354-9.354a.5.5 0 0 0-.708-.708L8 7.293 5.354 4.646a.5.5 0 1 0-.708.708L7.293 8l-2.647 2.646a.5.5 0 0 0 .708.708L8 8.707l2.646 2.647a.5.5 0 0 0 .708-.708L8.707 8l2.647-2.646z" />
					</svg>

				</div>
				<form asp-action="Index" asp-controller="Customer">
					<div class="text-center">
						<h1>Sorry !</h1>
						<p>Payment Unsucessful</p>
						<button type="submit" class="btn btn-outline-danger">Back Home</button>
					</div>
				</form>
			</div>
		</div>
	</div>

	<script src="~/js/jquery.min.js"></script>
	<script src="~/js/jquery-migrate-3.0.1.min.js"></script>
	<script src="~/js/popper.min.js"></script>
